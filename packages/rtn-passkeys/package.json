{
	"name": "@aws-amplify/rtn-passkeys",
	"version": "0.1.0",
	"description": "React Native module for aws-amplify webauthn support",
	"source": "./src/index.ts",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.mjs",
	"react-native": "./src/index.ts",
	"typings": "./dist/esm/index.d.ts",
	"sideEffects": false,
	"files": [
		"src",
		"lib",
		"android",
		"ios",
		"cpp",
		"*.podspec",
		"react-native.config.js",
		"!ios/build",
		"!android/build",
		"!android/gradle",
		"!android/gradlew",
		"!android/gradlew.bat",
		"!android/local.properties",
		"!**/__tests__",
		"!**/__fixtures__",
		"!**/__mocks__",
		"!**/.*"
	],
	"scripts": {
		"test": "jest",
		"typecheck": "tsc",
		"build:esm-cjs": "rollup --forceExit -c rollup.config.mjs",
		"build": "npm run clean && npm run build:esm-cjs",
		"lint": "eslint \"**/*.{js,ts,tsx}\"",
		"clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aws-amplify/amplify-js.git"
	},
	"author": "Amazon Web Services",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/aws/aws-amplify/issues"
	},
	"homepage": "https://docs.amplify.aws/",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"devDependencies": {
		"del-cli": "^5.1.0",
		"react-native": "^0.75.0",
		"typescript": "^5.2.2"
	},
	"peerDependencies": {
		"react": "*",
		"react-native": "*"
	},
	"jest": {
		"preset": "react-native",
		"modulePathIgnorePatterns": [
			"<rootDir>/example/node_modules",
			"<rootDir>/lib/"
		]
	},
	"codegenConfig": {
		"name": "AmplifyRtnPasskeysSpec",
		"type": "modules",
		"jsSrcsDir": "src",
		"outputDir": {
			"ios": "ios/generated",
			"android": "android/generated"
		},
		"android": {
			"javaPackageName": "com.amazonaws.amplify.rtnpasskeys"
		},
		"includesGeneratedCode": true
	}
}